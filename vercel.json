{
    "version": 2,
    "builds": [
        {
            "src": "utilitycoverapi/*.py",
            "use": "@vercel/python",
            "config" : {
                "maxLambdaSize": "50mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "utilitycoverapi/fastapidebit.py"
        }
    ]
}